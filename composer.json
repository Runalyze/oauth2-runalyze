{
  "name": "runalyze/php-oauth2-runalyze",
  "description": "Runalyze OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Pohl",
      "email": "michael@runalyze.com",
      "homepage": "https://runalyze.com"
    }
  ],
  "keywords": [
    "oauth",
    "oauth2",
